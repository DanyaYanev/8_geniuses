import math
formula = int(input("Введите номер задания : "))
result = 0
match formula:      # создание кейсов
    case 1:
        print("Путь,время,скорость.\n Основная формула: S = v * t ")
        finding = str(input("Введите искомое: "))
        if finding == "S":
            v = int(input("Введите скорость : "))
            t = int(input("Введите время : "))
            result = v * t
        elif finding == "v":
            S = int(input("Введите путь : "))
            t = int(input("Введите время : "))
            result = S/t
        elif finding == "t":
            S = int(input("Введите путь : "))
            v = int(input("Введите скорость : "))
            result = S/v
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 2:
        print("Равномерно ускоренное движение.Ускорение.\n Основная формула: a = Δv/t ")
        finding = str(input("Введите искомое: "))
        if finding == "a":
            delv = int(input("Введите изменение скорости, если оно не известно введите 0: "))
            if delv == 0:
                v0 = int(input("Введите нулевую скорость: "))
                v1 = int(input("Введите  скорость: "))
            t = int(input("Введите время : "))
            result = delv / t
        elif finding == "v":
            a = int(input("Введите ускорение : "))
            t = int(input("Введите время : "))
            result = a * t
        elif finding == "t":
            a = int(input("Введите ускорение : "))
            delv = int(input("Введите изменение скорости : "))
            if delv == 0:
                v0 = int(input("Введите нулевую скорость: "))
                v1 = int(input("Введите  скорость: "))
            result = delv/a
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 3:
        print("Равномерно ускоренное движение.Путь.\n Основная формула: S = v * t + (a * t^2) /2 ")
        v = int(input("Введите  скорость: "))
        t = int(input("Введите время : "))
        a = int(input("Введите ускорение : "))
        result = v * t + (a * (t ** 2)) / 2
        print(f"Ответ:{result}")
    case 4:
        print("Высота тела брошенного вертикально вверх. Основная формула: h = h0 + v0 * t - (g * t^2) /2 ")
        h0 = int(input("Введите изначальную высоту: "))
        v0 = int(input("Введите изначальную скорость: "))
        t = int(input("Введите время: "))
        g = 9.8
        result = h0 + v0 * t - (g * (t**2))/2
        print(f"Ответ:{result}")
    case 5:
        print("Скорость тела брошенного вертикально вверх. Основная формула: v =  v0 - g * t  ")
        v0 = int(input("Введите изначальную скорость: "))
        t = int(input("Введите время: "))
        g = 9.8
        result = v0 - g * t
        print(f"Ответ:{result}")
    case 6:
        print("Скорость, ускорение, время. Основная формула: v =  a * t  ")
        finding = str(input("Введите искомое: "))
        if finding == "a":
            v = int(input("Введите скорость : "))
            t = int(input("Введите время : "))
            result = v / t
        elif finding == "v":
            a = int(input("Введите ускорение : "))
            t = int(input("Введите время : "))
            result = a * t
        elif finding == "t":
            a = int(input("Введите ускорение : "))
            v = int(input("Введите скорость : "))
            result = v/a
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 7:
        print("Центростремительное ускорение. Основная формула: a = v^2/R  ")
        finding = str(input("Введите искомое: "))
        if finding == "a":
            v = int(input("Введите скорость : "))
            R = int(input("Введите радиус : "))
            result = v**2 / R
        elif finding == "v":
            a = int(input("Введите ускорение : "))
            R = int(input("Введите радиус : "))
            result = math.sqrt(a * R)
        elif finding == "R":
            a = int(input("Введите ускорение : "))
            v = int(input("Введите скорость : "))
            result = v**2 / a
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 8:
        print("Угловая скорость. Основная формула: ω = φ/t  ")
        finding = str(input("Введите искомое: "))
        if finding == "ω":
            fi = int(input("Введите угол : "))
            t = int(input("Введите время : "))
            result = fi/t
        elif finding == "φ":
            omega = int(input("Введите угловую скорость : "))
            t = int(input("Введите время : "))
            result = omega * t
        elif finding == "t":
            fi = int(input("Введите угол : "))
            omega = int(input("Введите угловую скорость : "))
            result = fi / omega
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 9:
        print("Равномерное круговое движение. Основная формула: l = R * φ ")
        R = int(input("Введите радиус: "))
        fi = int(input("Введите угол: "))
        result = R * fi
        print(f"Ответ:{result}")
    case 10:
        print("Равномерное круговое движение: линейная скорость. Основная формула: v = R * ω ")
        finding = str(input("Введите искомое: "))
        if finding == "v":
            omega = int(input("Введите угловую скорость : "))
            R = int(input("Введите радиус : "))
            result = R * omega
        elif finding == "ω":
            v = int(input("Введите линейную скорость : "))
            R = int(input("Введите радиус : "))
            result = v/R
        elif finding == "R":
            v = int(input("Введите линейную скорость : "))
            omega = int(input("Введите угловую скорость : "))
            result = v/omega
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 11:
        print("Период вращения1. Основная формула: T = t/N ")
        t = int(input("Введите время: "))
        N = int(input("Введите число вращений: "))
        result = t/N
        print(f"Ответ:{result}")
    case 12:
        print("Период вращения2. Основная формула: T = 2*π*R /v  ")
        pi = math.pi
        R = int(input("Введите радиус: "))
        v = int(input("Введите линейную скорость: "))
        result = (2 * pi * R) / v
        print(f"Ответ:{result}")
    case 13:
        print("Период вращения3. Основная формула: T = 2*π/ω  ")
        pi = math.pi
        omega = int(input("Введите угловую скорость: "))
        result = (2 * pi) / omega
        print(f"Ответ:{result}")
    case 14:
        print("Центростремительное ускорение. Основная формула: a = 4 * π^2 * R * n^2  ")
        pi = math.pi
        R = int(input("Введите радиус: "))
        n = int(input("Введите частоту вращения: "))
        result = 4 * pi**2 * R * n ** 2
        print(f"Ответ:{result}")
    case 15:
        print("Частота вращения. Основная формула: n = 1/T ")
        finding = str(input("Введите искомое: "))
        if finding == "n":
            T = int(input("Введите период вращения : "))
            result = 1/T
        elif finding == "T":
            n = int(input("Введите частоту вращения : "))
            result = 1/n
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 16:
        print("Центростремительное ускорение. Основная формула: a = ω^2 * R  ")
        R = int(input("Введите радиус: "))
        omega = int(input("Введите угловую скорость: "))
        result = omega ** 2 * R
        print(f"Ответ:{result}")
    case 17:
        print("Основное уравнение молекулярно-кинетической теории. Основная формула: p = 1/3 * n * m0 * v^2  ")
        n = int(input("Введите концентрацию: "))
        m0 = int(input("Введите массу молекулы: "))
        v = int(input("Введите скорость: "))
        result = 1/3 * n * m0 * v**2
        print(f"Ответ:{result}")
    case 18:
        print("Концентрация молекул. Основная формула: n = N/V  ")
        N = int(input("Введите число молекул: "))
        V = int(input("Введите объём: "))
        result = N/V
        print(f"Ответ:{result}")
    case 19:
        print("Давление идеального газа. Основная формула: p = 2/3 * n * E  ")
        E = int(input("Введите энергию: "))
        n = int(input("Введите концентрацию: "))
        result = 2/3 * n * E
        print(f"Ответ:{result}")
    case 20:
        print("Внутренняя энергия молекул. Основная формула: E = m * v^2 /2  ")
        m = int(input("Введите массу: "))
        v = int(input("Введите скорость: "))
        result = m * (v ** 2) / 2
        print(f"Ответ:{result}")
    case 21:
        print("Средняя кинетическая энергия. Основная формула: E  = (3 * k * T)/2 ")
        k = 1.36 * 10 ** -23
        T = int(input("Введите температуру: "))
        result = (3 * k * T)/2
        print(f"Ответ:{result}")
    case 22:
        print("Уравнение состояния идеального газа. Основная формула: p = vRT/V ")
        R = 8.3
        T = int(input("Введите температуру: "))
        V = int(input("Введите объём: "))
        v = int(input("Введите скорость: "))
        result = (v * R * T) / V
        print(f"Ответ:{result}")
