import math
formula = int(input("Введите номер задания : "))
match formula:
    case 1:
        result = 0
        k = 9 * 10 ** 9
        print("Закон Кулона.\nОсновная формула: F = k * (|q1| * |q2|) / r^2 ")
        finding = str(input("Введите искомое: "))
        if finding == "F":
            q1 = float(input("Введите первый заряд : "))
            q2 = float(input("Введите второй заряд : "))
            r = float(input("Введите расстояние между зарядами : "))
            result = (k * abs(q1 * q2)) / r ** 2
        elif finding == "k":
            result = k
        elif finding == "q1":
            F = float(input("Введите силу : "))
            q2 = float(input("Введите второй заряд : "))
            r = float(input("Введите расстояние между зарядами : "))
            result = (F * r ** 2) / (k * abs(q2))
        elif finding == "q2":
            F = float(input("Введите силу : "))
            q1 = float(input("Введите первый заряд : "))
            r = float(input("Введите расстояние между зарядами : "))
            result = (F * r ** 2) / (k * abs(q1))
        elif finding == "r":
            F = float(input("Введите силу : "))
            q1 = float(input("Введите первый заряд : "))
            q2 = float(input("Введите второй заряд : "))
            result = math.sqrt(k * abs(q1 * q2) * F)
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 2:
        result = 0
        k = 9 * 10 ** 9
        print("Плотность энергии электрического поля.\nФормула: Wp = k * (|q1| * |q2|) / r")
        finding = str(input("Введите искомое: "))
        if finding == "Wp":
            q1 = float(input("Введите первый заряд : "))
            q2 = float(input("Введите второй заряд : "))
            r = float(input("Введите расстояние между зарядами : "))
            result = (k * abs(q1 * q2)) / r
        elif finding == "k":
            result = k
        elif finding == "q1":
            Wp = float(input("Введите плотность : "))
            q2 = float(input("Введите второй заряд : "))
            r = float(input("Введите расстояние между зарядами : "))
            result = (Wp * r) / (k * abs(q2))
        elif finding == "q2":
            Wp = float(input("Введите плотность : "))
            q1 = float(input("Введите первый заряд : "))
            r = float(input("Введите расстояние между зарядами : "))
            result = (Wp * r) / (k * abs(q1))
        elif finding == "r":
            Wp = float(input("Введите плотность : "))
            q1 = float(input("Введите первый заряд : "))
            q2 = float(input("Введите второй заряд : "))
            result = k * abs(q1 * q2) * Wp
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 3:
        result = 0
        print("Плотность энергии электрического поля.\nФормула: Wp = Fd")
        finding = str(input("Введите искомое: "))
        if finding == "Wp":
            F = float(input("Введите силу : "))
            d = float(input("Введите расстояние между пластин : "))
            result = F * d
        elif finding == "F":
            Wp = float(input("Введите плотность : "))
            d = float(input("Введите расстояние между пластин : "))
            result = Wp / d
        elif finding == "d":
            Wp = float(input("Введите плотность : "))
            F = float(input("Введите силу : "))
            result = Wp / F
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 4:
        result = 0
        print("Плотность энергии электрического поля.\nФормула: Wp = qEd")
        finding = str(input("Введите искомое: "))
        if finding == "Wp":
            q = float(input("Введите заряд : "))
            E = float(input("Введите напряжённость электрического поля : "))
            d = float(input("Введите расстояние между пластин : "))
            result = q * E * d
        elif finding == "q":
            Wp = float(input("Введите плотность : "))
            E = float(input("Введите напряжённость электрического поля : "))
            d = float(input("Введите расстояние между пластин : "))
            result = Wp * E * d
        elif finding == "E":
            Wp = float(input("Введите плотность : "))
            q = float(input("Введите заряд : "))
            d = float(input("Введите расстояние между пластин : "))
            result = Wp * q * d
        elif finding == "d":
            q = float(input("Введите заряд : "))
            E = float(input("Введите напряжённость электрического поля : "))
            Wp = float(input("Введите плотность : "))
            result = q * E * Wp
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 5:
        result = 0
        print("Плотность энергии электрического поля.\nФормула: Wp = qφ / 2")
        finding = str(input("Введите искомое: "))
        if finding == "Wp":
            q = float(input("Введите заряд : "))
            φ = float(input("Введите потенциал : "))
            result = q * φ / 2
        elif finding == "q":
            Wp = float(input("Введите плотность : "))
            φ = float(input("Введите потенциал : "))
            result = Wp * 2 / φ
        elif finding == "φ":
            Wp = float(input("Введите плотность : "))
            q = float(input("Введите заряд : "))
            result = Wp * 2 / q
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 6:
        result = 0
        print("Потенциал электростатического поля.\nФормула: φ = Wp / q")
        finding = str(input("Введите искомое: "))
        if finding == "φ":
            Wp = float(input("Введите плотность : "))
            q = float(input("Введите заряд : "))
            result = Wp / q
        elif finding == "Wp":
            φ = float(input("Введите потенциал : "))
            q = float(input("Введите заряд : "))
            result = φ * q
        elif finding == "q":
            Wp = float(input("Введите плотность : "))
            φ = float(input("Введите потенциал : "))
            result = Wp / φ
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 7:
        result = 0
        k = 9 * 10 ** 9
        print("Потенциал электростатического поля.\nФормула: φ = k * q / r")
        finding = str(input("Введите искомое: "))
        if finding == "φ":
            q = float(input("Введите заряд : "))
            r = float(input("Введите расстояние между зарядами : "))
            result = k * q / r
        elif finding == "k":
            result = k
        elif finding == "q":
            φ = float(input("Введите потенциал : "))
            r = float(input("Введите расстояние между зарядами : "))
            result = φ * r / k
        elif finding == "r":
            q = float(input("Введите заряд : "))
            φ = float(input("Введите потенциал : "))
            result = k * q / φ
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 8:
        result = 0
        k = 9 * 10 ** 9
        print("Потенциал электростатического поля.\nФормула: φ = Ed")
        finding = str(input("Введите искомое: "))
        if finding == "φ":
            E = float(input("Введите напряжённость электрического поля : "))
            d = float(input("Введите расстояние между пластин : "))
            result = E * d
        elif finding == "E":
            φ = float(input("Введите потенциал : "))
            d = float(input("Введите расстояние между пластин : "))
            result = φ * d
        elif finding == "d":
            E = float(input("Введите напряжённость электрического поля : "))
            φ = float(input("Введите потенциал : "))
            result = E * φ
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 9:
        result = 0
        print("Работа в электрическом поле.\nФормула: A = W1 - W2")
        finding = str(input("Введите искомое: "))
        if finding == "A":
            W1 = float(input("Введите начальную потенциальную энергию : "))
            W2 = float(input("Введите конечную потенциальную энергию : "))
            result = W1 - W2
        elif finding == "W1":
            A = float(input("Введите работу : "))
            W2 = float(input("Введите конечную потенциальную энергию : "))
            result = A + W2
        elif finding == "W2":
            A = float(input("Введите работу : "))
            W1 = float(input("Введите начальную потенциальную энергию : "))
            result = W1 - A
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 10:
        result = 0
        print("Работа в электрическом поле.\nФормула: A = q(φ1 - φ2")
        finding = str(input("Введите искомое: "))
        if finding == "A":
            q = float(input("Введите заряд : "))
            φ1 = float(input("Введите начальный потенциал : "))
            φ2 = float(input("Введите конечный потенциал : "))
            result = q * (φ1 - φ2)
        elif finding == "q":
            A = float(input("Введите работу : "))
            φ1 = float(input("Введите начальный потенциал : "))
            φ2 = float(input("Введите конечный потенциал : "))
            result = A / (φ1 - φ2)
        elif finding == "φ1":
            A = float(input("Введите работу : "))
            φ2 = float(input("Введите конечный потенциал : "))
            q = float(input("Введите заряд : "))
            result = (A + φ2 * q) / q
        elif finding == "φ2":
            A = float(input("Введите работу : "))
            q = float(input("Введите заряд : "))
            φ1 = float(input("Введите начальный потенциал : "))
            result = (φ1 * q - A )/ q
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 11:
        result = 0
        print("Электрическая ёмкость двух проводников.\nФормула: C = q / U")
        finding = str(input("Введите искомое: "))
        if finding == "C":
            q = float(input("Введите заряд : "))
            U = float(input("Введите напряжение : "))
            result = q / U
        elif finding == "q":
            C = float(input("Введите электрическую ёмкость : "))
            U = float(input("Введите напряжение : "))
            result = C * U
        elif finding == "U":
            C = float(input("Введите электрическую ёмкость : "))
            q = float(input("Введите заряд : "))
            result = C * q
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 12:
        result = 0
        ε = 1
        ε0 = 8.85 * 10 ** -12
        print("Электрическая ёмкость плоского конденсатора.\nФормула: C = ε * ε0 * S / d")
        finding = str(input("Введите искомое: "))
        if finding == "C":
            S = float(input("Введите площадь : "))
            d = float(input("Введите расстояние между пластин : "))
            result = ε * ε0 * S / d
        elif finding == "ε":
            result = ε
        elif finding == "ε0":
            result = ε0
        elif finding == "d":
            S = float(input("Введите площадь : "))
            C = float(input("Введите электрическую ёмкость : "))
            result = ε * ε0 * S / C
        elif finding == "s":
            C = float(input("Введите электрическую ёмкость : "))
            d = float(input("Введите расстояние между пластин : "))
            result = C * d / ε * ε0
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
