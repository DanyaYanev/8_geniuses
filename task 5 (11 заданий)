tasks = int(input("Введите номер задания :"))
match tasks:      # создание кейсов
    case 1:
        print("a:", (101 + 0) / 3)
        print("b:", 3.0e-6 * 10000000.1)
        print("c:", True and True)
        print("d:", False and True)
        print("e:", (False and False) * (True and True))
        print("f:", (False and False) and (True and True))
    case 2:
        number1, number2, number3, number4 = map(int, input("введите 4 числа через пробел: ").split())   # ввод 4 чисел
        if number4 == number3 == number2 == number1:  # сравнение чисел
           print("равно")    # вывод
        else:
           print("не равно")
    case 3:
        x = []     # создание пустого списка
        i = 0      # создание счётчика повторений
        n = int(input("Введите количество чисел: "))    # ввод количества чисел
        for i in range(i, n):    # создание цикла количества чисел
           numb = int(input("Введите число: "))  # ввод числа
           x.append(numb)  # добавление числа в список
        print(max(x))      # вывод максимального числа
    case 4:
        x = []   # создание пустого списка
        i = 0    # создание счётчика повторений
        n = int(input("Введите количество чисел: "))   # ввод количества чисел
        for i in range(i, n):         # создание цикла количества чисел
            numb = int(input("Введите число: "))       # ввод числа
            x.append(numb)                # добавление числа в список
        print(min(x))    # вывод минимального числа
    case 5:
        x = []        # создание пустого списка
        i = 0         # создание счётчика повторений
        n = int(input("Введите количество чисел: "))     # ввод количества чисел
        for i in range(n):       # создание цикла количества чисел
            numb = int(input("Введите число: "))        # ввод числа
            x.append(numb)         # добавление числа в список
        a = sum(x) / n       # Среднее арифметическое чисел
        for i in range(n):        # создание цикла сравнения чисел
            if x[i] > a:    # Сравнение чисел со средним арифметическим
                print(x[i])    # вывод числа большего среднего арифметического
    case 6:
        number1, number2 = map(int, input("введите 2 числа через пробел: ").split())      # ввод 2 чисел
        c = number1
        number2 = number2 - 1
        for i in range(number2):      # создание цикла сложения чисел
            number1 = number1 + c     # сложение чисел(умножение)
        print(number1)          # вывод
    case 7:
        n = int(input("Введите количество цифр: "))   # ввод количества чисел
        even = []      # создание пустого списка
        odd = []       # создание пустого списка
        for i in range(n):   # создание цикла
            numbers = int(input("Введите число: "))   # ввод числа
            if numbers % 2 == 0:             # определение чётности числа через mod
                even.append(numbers)          # добавление числа в список
            else:
                odd.append(numbers)          # добавление числа в список
        print("Четные числа:", *even)       # вывод
        print("Нечетные числа:", *odd)      # вывод

    case 8:
        celsius = float(input("Введите температуру в градусах Цельсия:"))  # ввод температуры
        print("Температура в градусах Фарингейт:", celsius * 1.8 + 32)   # вывод температуры
    case 9:
        m = int(input("Введите массу:"))         # ввод массы
        h = int(input("Введите рост:"))          # ввод рост
        print("Индекс массы тела:", m / ((h/100) ** 2))  #вывод индекса массы тела
    case 10:
        number = int(input("Введите число:"))  # ввод числа 
        print("Ваше число в квадрате:", number ** 2)   # возведение числа в квадрат 
        print("Ваше число в кубе:", number ** 3)         # возведение числа в куб
        print("Ваше число в четвёртой степени:", number ** 4)     # возведение числа в четвёртую степень
    case 11:
        side1 = float(input("Введите первую сторону:"))     # ввод числа
        side2 = float(input("Введите вторую сторону:"))     # ввод числа
        side3 = float(input("Введите третью сторону:"))     # ввод числа
        if side1 + side2 > side3 and side2 + side3 > side1 and side3 + side1 > side2:   # сравнение сторон на возможность существования треугольника
            print("треугольник может существовать")    # вывод
        else:
            print("треугольник не может существовать")    # вывод
