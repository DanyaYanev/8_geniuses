#1 

print((101 + 0) / 3)

print(3.0e-6 * 10000000.1)

print(True and True)

print(False and False)

print((False and False) or (True and True))

print((False or False) and (True and True))

#2

a, b, c, d = int(input("Введите число a: ")), int(input("Введите число b: ")), int(input("Введите число c: ")), int(input("Введите число d: "))
if a == b and a == c and a == d:
    print("Равно")
else:
    print("Не равно")

#3

import random
k = int(input())
n = 30
c = [0] * n
for i in range(n):
    c[i] = random.randint(1, 100)
print(c)
print(sorted(c, reverse=True)[:k])

#4

import random
k = int(input())
n = 30
c = [0] * n
for i in range(n):
    c[i] = random.randint(1, 100)
print(c)
print(sorted(c)[:k])

#5

import random
k = int(input())
n = 30
c = [0] * n
for i in range(n):
    c[i] = random.randint(1, 100)
print(c)
m = max(c)
l = min(c)
mid = (m+l) / 2
print(mid)
print(sorted(c)[:k])

#6

a, b = int(input("Введите число a: ")), int(input("Введите число b: "))
count = 0
for i in range(b):
    count+= a
    b -=1
print(count)
    
#8 

far = int(input("Введите температуру в Фаренгейтах: ")) 
c = (far - 32) / 1.8
print(c)

#9

mass, height = float(input("Введите массу тела: ")), float(input("Введите рост (в метрах): "))
ind = mass / height ** 2
print(ind)

#10

num = float(input("Введите число: "))
square = num ** 2
cube = num ** 3
four = num **4
print(square, cube, four, sep = "\n")

#11

a, b, c = float(input("Введите сторону a: ")), float(input("Введите сторону b: ")), float(input("Введите сторону c: "))
if a < (b + c) and b < (a+c) and c < (a + b):
    print("Треугольник существует")
else:
    print("Треугольника с такими сторонами не существует")
