spisok1 = [5,4,9,124,6]
print( spisok1)
spisok2 = [12,68,64,23,2,17]
print(spisok2)
spisok3 = []
n = 5
for i in range(n):
    number = spisok1[i]
    if number % 2 == 0:
        spisok3.append(number)
n = 6
for i in range(n):
    number = spisok2[i]
    if number % 2 != 0:
        spisok3.append(number)
print(spisok3)

